{
    "arguments": [ "fedora_host", "fedora_gcc_m64_dir" ],
    "machine": "$fedora_host",
    "depends": [ "./make" ],
    "workdir": "$fedora_gcc_m64_dir/tests",
    "execute": [ "cp -rT qemu-iotests $__fname",
                 "cd $__fname && TEST_DIR=/tmp/$__fname ./check -T -c writethrough -qcow2 -o data_file='$$TEST_IMG.ext_data_file'" ],
    "failcount": 3
}
