{
    "arguments": [ "fedora_host", "fedora_gcc_m32_dir" ],
    "machine": "$fedora_host",
    "variable_jobs": true,
    "depends": [ "./clean" ],
    "workdir": "$fedora_gcc_m32_dir",
    "execute": [ "PKG_CONFIG_LIBDIR=/usr/lib/pkgconfig make -j $__jobs" ],
    "threads": "$__jobs",
    "failcount": 1
}
