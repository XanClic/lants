{
    "arguments": [ "arch_host", "arch_clang_m32_dir" ],
    "machine": "$arch_host",
    "depends": [ "./make" ],
    "workdir": "$arch_clang_m32_dir/tests",
    "execute": [ "cp -rT qemu-iotests $__fname",
                 "cd $__fname && TEST_DIR=/tmp/$__fname ./check -T -c writethrough -vmdk" ],
    "failcount": 3
}
